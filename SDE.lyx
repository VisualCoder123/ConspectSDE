\begin_layout Enumerate
\begin_inset Formula 
\begin{multline*}
E\left|I\left(A\right)\right|^{2}=E\left[\sum A\left(s_{k}\right)\Delta_{s_{k}}\omega\right]^{2}=\\
=E\sum\left(A\left(s_{k}\right)\Delta_{s_{k}}\omega\right)^{2}+E\left[\sum\sum A\left(t_{k}\right)\Delta_{t_{k}}\omega\cdot A\left(s_{k}\right)\Delta_{s_{k}}\omega\right]^{2}+E\left[\sum\sum A\left(s_{k}\right)\Delta_{s_{k}}\omega\cdot A\left(s_{j}\right)\Delta_{s_{j}}\omega\right]=\\
=E\sum E\left[\left[A\left(S_{k}\right)\Delta_{s_{k}}\omega\right]^{2}|{\cal F}_{s_{k}}\right]=E\sum A^{2}\left(s_{k}\right)E\left[\Delta_{s_{k}}\omega\right]^{2}|{\cal F}_{s_{k}}=E\sum A^{2}\left(s_{k}\right)\left[s_{k+1}-s_{k}\right]
\end{multline*}

\end_inset


\begin_inset Formula 
